@model IEnumerable<BendeYaparim.Web.Models.Category>
<span class="myTitle">En cok eleman aranan kategoriler.</span>
<div class="clear">
</div>
<p class="SubCont turkuaz">
    @foreach (var item in Model)
    {
        <a href="@Url.Action("Index", "JobOffer", new { CategoryId = item.Id, CategoryName = BendeYaparim.Web.CategoryNameConverterToUrl.ToFriendlyUrl(item.Name) })" class="t2">
            @item.Name (@item.NumberOfJobOffers)</a> 
    }
</p>
